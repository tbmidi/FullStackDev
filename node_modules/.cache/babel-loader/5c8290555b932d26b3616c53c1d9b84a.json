{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { ContentLoader } from \"vue-content-loader\";\nexport default {\n  components: {\n    ContentLoader\n  },\n\n  setup() {\n    const store = useStore();\n    onMounted(() => {\n      store.dispatch('slider/getSliders');\n    });\n    const sliders = computed(() => {\n      return store.state.slider.sliders;\n    });\n    const isLoading = computed(() => {\n      return store.state.slider.isLoading;\n    });\n    return {\n      sliders,\n      isLoading\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiDA,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,aAAT,QAA8B,oBAA9B;AAEA,eAAe;AACXC,YAAU,EAAE;AACRD;AADQ,GADD;;AAIXE,OAAK,GAAG;AACJ,UAAMC,KAAI,GAAIJ,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZK,WAAK,CAACC,QAAN,CAAe,mBAAf;AACH,KAFQ,CAAT;AAIA,UAAMC,OAAM,GAAIR,QAAQ,CAAC,MAAM;AAC3B,aAAOM,KAAK,CAACG,KAAN,CAAYC,MAAZ,CAAmBF,OAA1B;AACH,KAFuB,CAAxB;AAIA,UAAMG,SAAQ,GAAIX,QAAQ,CAAC,MAAM;AAC7B,aAAOM,KAAK,CAACG,KAAN,CAAYC,MAAZ,CAAmBC,SAA1B;AACH,KAFyB,CAA1B;AAIA,WAAO;AACHH,aADG;AAEHG;AAFG,KAAP;AAIJ;;AAvBW,CAAf","names":["computed","onMounted","useStore","ContentLoader","components","setup","store","dispatch","sliders","state","slider","isLoading"],"sourceRoot":"","sources":["/Users/benmehidilarbi/Desktop/online-shop-main/src/components/Slider.vue"],"sourcesContent":["<template>\n    <div v-if=\"isLoading\">\n        <content-loader\n            viewBox=\"0 0 500 250\"\n            :speed=\"2\"\n            primaryColor=\"#f3f3f3\"\n            secondaryColor=\"#ecebeb\"\n        >\n            <rect x=\"0\" y=\"0\" rx=\"5\" ry=\"5\" width=\"500\" height=\"250\" />\n        </content-loader>\n    </div>\n    <div\n        id=\"carouselExampleControls\"\n        class=\"carousel slide carousel-fade\"\n        data-bs-ride=\"carousel\"\n        v-else\n    >\n        <div class=\"carousel-inner\">\n            <div\n                class=\"carousel-item\"\n                v-for=\"(slider, id) in sliders\"\n                :class=\"{ active: id == 0 }\"\n                :key=\"slider.id\"\n            >\n                <img :src=\"slider.image\" class=\"d-block w-100 rounded\" />\n            </div>\n        </div>\n        <button\n            class=\"carousel-control-prev\"\n            type=\"button\"\n            data-bs-target=\"#carouselExampleControls\"\n            data-bs-slide=\"prev\"\n        >\n            <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n            <span class=\"visually-hidden\">Previous</span>\n        </button>\n        <button\n            class=\"carousel-control-next\"\n            type=\"button\"\n            data-bs-target=\"#carouselExampleControls\"\n            data-bs-slide=\"next\"\n        >\n            <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n            <span class=\"visually-hidden\">Next</span>\n        </button>\n    </div>\n</template>\n\n<script>\nimport { computed, onMounted } from 'vue'\nimport { useStore } from 'vuex'\nimport { ContentLoader } from \"vue-content-loader\"\n\nexport default {\n    components: {\n        ContentLoader\n    },\n    setup() {\n        const store = useStore()\n\n        onMounted(() => {\n            store.dispatch('slider/getSliders')\n        })\n\n        const sliders = computed(() => {\n            return store.state.slider.sliders\n        })\n\n        const isLoading = computed(() => {\n            return store.state.slider.isLoading\n        })\n\n        return {\n            sliders,\n            isLoading\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}