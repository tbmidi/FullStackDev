{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nexport default {\n  setup() {\n    const store = useStore();\n    const route = useRoute();\n    onMounted(() => {\n      store.dispatch('category/getProductInCategory', route.params.slug);\n    });\n    const products = computed(() => {\n      return store.state.category.productInCategory;\n    });\n    return {\n      store,\n      route,\n      products\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkEA,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,QAAyB,YAAzB;AAEA,eAAe;AACXC,OAAK,GAAG;AACJ,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AAEA,UAAMI,KAAI,GAAIH,QAAQ,EAAtB;AAEAF,aAAS,CAAC,MAAM;AACZI,WAAK,CAACE,QAAN,CAAe,+BAAf,EAAgDD,KAAK,CAACE,MAAN,CAAaC,IAA7D;AACH,KAFQ,CAAT;AAIA,UAAMC,QAAO,GAAIV,QAAQ,CAAC,MAAM;AAC5B,aAAOK,KAAK,CAACM,KAAN,CAAYC,QAAZ,CAAqBC,iBAA5B;AACH,KAFwB,CAAzB;AAIA,WAAO;AACHR,WADG;AAEHC,WAFG;AAGHI;AAHG,KAAP;AAKJ;;AAnBW,CAAf","names":["computed","onMounted","useStore","useRoute","setup","store","route","dispatch","params","slug","products","state","category","productInCategory"],"sourceRoot":"","sources":["/Users/benmehidilarbi/Desktop/online-shop-main/src/views/category/Show.vue"],"sourcesContent":["<template>\n    <div class=\"container-fluid mb-5 mt-4\" v-if=\"products.length >= 1\">\n        <div class=\"row gy-2\">\n            <div\n                v-for=\"product in products\"\n                :key=\"product.id\"\n                class=\"col-md-4 col-lg-3 col-12 mb-3\"\n            >\n                <div class=\"card border-0 shadow rounded-md\">\n                    <div class=\"card-img\">\n                        <img\n                            v-lazy=\"{ src: product.image }\"\n                            class=\"w-100\"\n                            style=\"height: 15em;object-fit:cover;border-top-left-radius: .25rem;border-top-right-radius: .25rem;\"\n                        />\n                    </div>\n                    <div class=\"card-body text-center\">\n                        <p class=\"card-title\" style=\"font-weight: bold;\">{{ product.title }}</p>\n                        <div\n                            class=\"discount mt-2 text-danger d-flex align-items-center justify-content-center\"\n                            v-if=\"product.discount > 0\"\n                        >\n                            <s>Rp. {{ moneyFormat(product.price) }}</s>\n                            <span\n                                style=\"background-color: darkorange\"\n                                class=\"badge badge-pill badge-success text-white ms-2\"\n                            >\n                                Diskon\n                                {{ product.discount }} %\n                            </span>\n                        </div>\n                        <div\n                            class=\"price fw-bold mt-3\"\n                            style=\"color: #47b04b;font-size:20px\"\n                        >Rp. {{ moneyFormat(calculateDiscount(product)) }}</div>\n                        <hr style=\"border-top: 1px solid rgb(0 0 0);border-radius:.5rem\" />\n                        <router-link\n                            :to=\"{ name: 'detail_product', params: { slug: product.slug } }\"\n                            class=\"btn btn-primary btn-md w-100 shadow-md\"\n                        >Lihat Produk</router-link>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"container mb-5 mt-4\" v-else>\n        <div class=\"row gy-2\">\n            <div class=\"col-12\">\n                <div class=\"card border-0 rounded-3 shadow\">\n                    <div class=\"p-4 text-center\">\n                        <img src=\"../../assets/empty-products.svg\" style=\"width: 250px\" />\n                        <p class=\"fs-4 mt-3 fw-bold\">Produits introuvables dans cette cat√©gorie.</p>\n                        <router-link\n                            :to=\"{ name: 'home' }\"\n                            class=\"btn btn-primary btn-lg shadow-md\"\n                        >\n                            <i class=\"fa fa-long-arrow-alt-left\"></i> Maison\n                        </router-link>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { computed, onMounted } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\n\nexport default {\n    setup() {\n        const store = useStore()\n\n        const route = useRoute()\n\n        onMounted(() => {\n            store.dispatch('category/getProductInCategory', route.params.slug)\n        })\n\n        const products = computed(() => {\n            return store.state.category.productInCategory\n        })\n\n        return {\n            store,\n            route,\n            products\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}